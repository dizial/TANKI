<Window x:Class="TANKI_client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TANKI_client"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Canvas x:Name="Field" Background="BlanchedAlmond" Grid.RowSpan="2" KeyDown="KeyDown" KeyUp="KeyUp" Focusable="True">
        <Rectangle x:Name="Tank" Canvas.Top = "20" Canvas.Left="20" Width="25" Height="55" Fill="Black" RadiusX="10" RadiusY="10"/>

    </Canvas>
</Window>
